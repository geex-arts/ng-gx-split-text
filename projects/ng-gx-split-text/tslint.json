{
  "extends": "../../tslint.json",
  "rules": {
    "max-line-length": false,
    "no-trailing-whitespace": false,
    "directive-selector": [
      true,
      "attribute",
      "lib",
      "camelCase",
      "ng"
    ],
    "component-selector": [
      true,
      "element",
      "lib",
      "kebab-case",
      "ng"
    ]
  }
}
